//dynamic plymrphsm-> overriding 

class Animal{

	
}

class Cat extends Animal
{
	void move() {
		System.out.println("CAT MOVES");
	}
	
}
class Tiger extends Cat
{

}
public class SciCalcService {

	public static void main(String[] args) {

		Tiger t=new Tiger();
		t.move();
	}

}
