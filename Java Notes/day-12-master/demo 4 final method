package pack1;

//parent comes b4 child ??
//child inherits everything frm parent except private and constructor

// inherit
//invoking

//super() must be the frst statemt in child const

//final methods cannot be overridden

class parent{

	final void car()
	{
	System.out.println("maruti 800");	
	}

	
}

class child extends parent{
	
	/*
	 * @Override void car() {
	 * 
	 * System.out.println("baleno"); super.car(); super.car(); }
	 */	
}


public class InhrtncDemo {

	public static void main(String[] args) {

		
		//parent p=new parent();
		child c=new child();
		c.car();
		
	}

}
