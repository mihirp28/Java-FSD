
// try , catch ,finally ,throw , throws

public class ExceptionsDemo {

	public static void main(String[] args) {

		try {
		System.out.println("line 3 "+(10/0));  
		}
		catch (ArithmeticException e) {
			e.printStackTrace();
			
		}
		
		System.out.println("line 1");
		System.out.println("line 2");
	
	}

}
