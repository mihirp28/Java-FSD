
//array of array

public class SciCalcService {

	public static void main(String[] args) {


	int[][] marks=new int[3][];	
		
	marks[0]=new int[] {58,9,77,65,49};
	marks[1]=new int[]{1,2,4,5};
	marks[2]=new int[]{1,2,4};
	
System.out.println(marks[0][0]);// ?? 58
System.out.println(marks[0][4]); // ??  49
System.out.println(marks[1][3]); // ?? 5	
System.out.println(marks[2][2]); // ?? 4


		}

}
// sslc
// a -> 50
// b  ->40
// c ->30
